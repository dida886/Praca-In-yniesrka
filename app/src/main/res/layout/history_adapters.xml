<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/tytul_adapter"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="120dp"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/data_adapter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tytul_adapter"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:textStyle="bold"
        android:text="TextView" />

    <TextView
        android:id="@+id/OPISTV"
        android:layout_width="256dp"
        android:layout_height="45dp"
        android:layout_below="@+id/tytul_adapter"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_toEndOf="@+id/imageView2"
        android:text="TextView" />

    <TextView
        android:id="@+id/adres_adapter"
        android:layout_width="200dp"
        android:layout_height="wrap_content"


        android:layout_alignStart="@+id/tytul_adapter"
        android:layout_alignBottom="@+id/imageView2"
        android:text="TextView" />

</RelativeLayout>